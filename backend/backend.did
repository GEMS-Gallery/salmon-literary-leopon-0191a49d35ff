type Transaction = 
 record {
   amount: int;
   description: text;
   timestamp: int;
 };
type Result_1 = 
 variant {
   err: text;
   ok: int;
 };
type Result = 
 variant {
   err: text;
   ok;
 };
type Investment = 
 record {
   name: text;
   trend: float64;
   value: int;
 };
type ExpenseCategory = 
 record {
   amount: int;
   category: text;
 };
service : {
  addFunds: (int) -> (Result_1);
  addInvestment: (text, int, float64) -> (Result);
  addTransaction: (int, text) -> (Result);
  getBalance: () -> (int) query;
  getExpensesData: () -> (vec ExpenseCategory) query;
  getInvestments: () -> (vec Investment) query;
  getRecentTransactions: () -> (vec Transaction) query;
  getSavingsGoalData: () -> (int, int) query;
}
